[ignore]
<PROJECT_ROOT>/node_modules/styled-components/.*
<PROJECT_ROOT>/node_modules/stylelint/.*
<PROJECT_ROOT>/node_modules/relay-compiler/.*
<PROJECT_ROOT>/node_modules/babel-plugin-relay/.*
<PROJECT_ROOT>/node_modules/babel-plugin-flow-runtime/.*
<PROJECT_ROOT>/build/.*
<PROJECT_ROOT>/temp/.*
<PROJECT_ROOT>/var/.*
<PROJECT_ROOT>/coverage/.*

[libs]

[options]
module.name_mapper='^~/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^types/\(.*\)$' -> '<PROJECT_ROOT>/src/types/\1'
module.name_mapper='^cc$' -> '<PROJECT_ROOT>/src/const/cc.js'
module.name_mapper='^version$' -> '<PROJECT_ROOT>/version.mock.js'
