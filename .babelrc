{
    "presets": [
        [
            "env",
            {
                "useBuiltIns": true,
                "targets": [
                    "chrome >= 59"
                ]
            }
        ],
        "react-optimize",
        "react",
        "stage-0"
    ],
    "plugins": [
        "trace",
        "flow-runtime",
        "partial-application",
        "idx",
        "ramda",
        "react-require",
        "styled-components"
    ],
    "env": {
        "production": {
            "presets": [
                [
                    "env",
                    {
                        "useBuiltIns": true,
                        "targets": [
                            "last 2 versions"
                        ]
                    }
                ]
            ],
            "plugins": [
                [
                    "groundskeeper-willie",
                    {
                        "removeConsole": true,
                        "removeDebugger": true,
                        "removePragma": false
                    }
                ]
            ]
        }
    }
}
